        "zendframework/zend-http": "2.*",
        "zendframework/zend-loader": "2.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-view": "2.*"
    },
    "require-dev": {
